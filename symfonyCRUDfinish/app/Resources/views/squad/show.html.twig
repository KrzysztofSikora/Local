{% extends 'base.html.twig' %}

{% block body %}
    <h1>Squad</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ squad.id }}</td>
            </tr>
            <tr>
                <th>Filenameee</th>
                <td>{{ squad.filenameee }}</td>
            </tr>
            <tr>
                <th>Mimeee</th>
                <td>{{ squad.mimeee }}</td>
            </tr>
            <tr>
                <th>Contentsss</th>
                <td>{{ squad.contentsss }}</td>
            </tr>
            <tr>
                <th>Descriptionnn</th>
                <td>{{ squad.descriptionnn }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ squad.name }}</td>
            </tr>
            <tr>
                <th>Info</th>
                <td>{{ squad.info }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('squad_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('squad_edit', { 'id': squad.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
